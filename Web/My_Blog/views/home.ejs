<!-- nho de dau cham phay dang sau header -->
<%- include('partials/header'); -%>

<h1>HOME</h1>
<p><%- startingContent %></p>
<% posts.forEach(function(post) { %>
<h1><%= post.title %> </h1>
<p><%= post.content.substring(0,100)+"..." %>
    <!-- // ejg co the dinh kem trong string -->
    <a href="/posts/<%= post.title %> " class="RM-btn">Read more</a>
</p>
<% }) %>
<%- include('partials/footer'); -%>